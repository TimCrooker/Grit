{
	"name": "grit-cli",
	"version": "0.0.1",
	"description": "Simple and flexible scaffolding",
	"main": "dist/index.js",
	"bin": {
		"grit": "cli.js"
	},
	"types": "dist/index.d.ts",
	"files": [
		"dist",
		"/cli.js",
		"!**/*.test.ts"
	],
	"scripts": {
		"start": "yarn build && ts-node cli.js",
		"dev": "ts-node ./src/cliEngine.ts",
		"build": "rimraf dist && tsup src/index.ts --dts",
		"test": "jest",
		"lint": "eslint . --fix",
		"prepublishOnly": "yarn build",
		"release": "dotenv release-it --ci",
		"release:manual": "dotenv release-it --only-version"
	},
	"license": "MIT",
	"engines": {
		"node": ">=12"
	},
	"dependencies": {
		"rimraf": "^3.0.2",
		"update-notifier": "^5.1.0"
	},
	"devDependencies": {
		"@babel/cli": "^7.14.8",
		"@babel/core": "^7.15.5",
		"@babel/plugin-transform-runtime": "^7.15.0",
		"@babel/preset-env": "^7.15.4",
		"@babel/preset-typescript": "^7.15.0",
		"@egoist/extract-zip": "^2.0.1",
		"@types/cross-spawn": "^6.0.2",
		"@types/ejs": "^3.0.4",
		"@types/fs-extra": "^9.0.1",
		"@types/hash-sum": "^1.0.0",
		"@types/ini": "^1.3.30",
		"@types/jest": "^25.2.2",
		"@types/micromatch": "^4.0.1",
		"@types/mkdirp": "^1.0.2",
		"@types/move-file": "^1.1.0",
		"@types/node": "^16.7.10",
		"@types/parse-package-name": "^0.1.0",
		"@types/rimraf": "^3.0.2",
		"@types/text-table": "^0.2.1",
		"@types/update-notifier": "^4.1.0",
		"@typescript-eslint/eslint-plugin": "^2.33.0",
		"@typescript-eslint/parser": "^2.33.0",
		"axios": "^0.24.0",
		"babel-jest": "^27.1.0",
		"babel-plugin-module-resolver": "^4.1.0",
		"cac": "^6.5.13",
		"chalk": "^4.1.0",
		"cli-table3": "^0.6.0",
		"cross-spawn": "^7.0.3",
		"cz-conventional-changelog": "3.2.0",
		"dot-prop": "^5.2.0",
		"dotenv-cli": "^4.0.0",
		"ejs": "^3.1.2",
		"enquirer": "^2.3.6",
		"env-paths": "^2.2.0",
		"esbuild": "^0.13.10",
		"eslint": "^7.32.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-prettier": "^4.0.0",
		"fs-extra": "^9.0.1",
		"hash-sum": "^2.0.0",
		"husky": "^7.0.4",
		"ini": "^1.3.5",
		"is-binary-path": "^2.1.0",
		"jest": "^26.0.1",
		"joycon": "^2.2.5",
		"lint-staged": "^10.2.2",
		"log-update": "^4.0.0",
		"majo": "^0.10.1",
		"micromatch": "^4.0.2",
		"move-file": "^2.0.0",
		"ora": "^4.0.4",
		"parse-package-name": "^0.1.0",
		"prettier": "^2.3.2",
		"release-it": "^14.11.6",
		"resolve-from": "^5.0.0",
		"rollup": "^2.34.0",
		"text-table": "^0.2.0",
		"ts-jest": "^26.0.0",
		"ts-node": "^8.10.2",
		"tsup": "^5.5.0",
		"type-fest": "^2.5.2",
		"typescript": "^4.1.2",
		"yarn-global": "^1.2.0"
	},
	"config": {
		"commitizen": {
			"path": "./node_modules/cz-conventional-changelog"
		}
	},
	"keywords": [
		"generator",
		"yeoman",
		"template",
		"scaffold",
		"automation",
		"simple",
		"easy",
		"workflow"
	]
}
