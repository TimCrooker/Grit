{
	"name": "grit-cli",
	"version": "0.5.8",
	"description": "Simple and flexible scaffolding",
	"bin": {
		"grit": "dist/index.js"
	},
	"files": [
		"dist",
		"!**/*.(test|spec).ts"
	],
	"author": "Tim Crooker <timothycrooker@gmail.com>",
	"homepage": "https://github.com/TimCrooker/grit#readme",
	"main": "lib/grit-cli.js",
	"directories": {
		"lib": "lib",
		"test": "__tests__"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/TimCrooker/grit.git"
	},
	"bugs": {
		"url": "https://github.com/TimCrooker/grit/issues"
	},
	"scripts": {
		"start": "npm run build && npm run cli",
		"build:watch": "nodemon --watch src -e ts --ignore dist --exec npm run build",
		"cli": "node ./dist/index.js",
		"build": "npm run clean && tsup src/index.ts --dts",
		"clean": "rimraf ./dist",
		"test": "jest",
		"lint": "eslint . --fix",
		"prepublishOnly": "npm run build",
		"release": "dotenv release-it",
		"release:ci": "dotenv release-it --ci"
	},
	"license": "MIT",
	"engines": {
		"node": ">=14"
	},
	"dependencies": {
		"axios": "^0.24.0",
		"clifi": "^1.2.4",
		"commander": "^8.3.0",
		"dot-prop": "^5.3.0",
		"figlet": "^1.5.2",
		"fs-extra": "^9.1.0",
		"gritenv": "^0.3.0",
		"ini": "^2.0.0",
		"inquirer": "^8.2.0",
		"ora": "4.1.1",
		"resolve-from": "^5.0.0",
		"rimraf": "^3.0.2",
		"swaglog": "^0.0.0",
		"update-notifier": "^5.1.0"
	},
	"devDependencies": {
		"@babel/cli": "^7.14.8",
		"@babel/core": "^7.15.5",
		"@babel/plugin-transform-runtime": "^7.15.0",
		"@babel/preset-env": "^7.15.4",
		"@babel/preset-typescript": "^7.15.0",
		"@egoist/extract-zip": "^2.0.1",
		"@types/chokidar": "^2.1.3",
		"@types/commander": "^2.12.2",
		"@types/figlet": "^1.5.4",
		"@types/fs-extra": "^9.0.1",
		"@types/ini": "^1.3.31",
		"@types/inquirer": "^8.1.3",
		"@types/jest": "^25.2.2",
		"@types/node": "^16.7.10",
		"@types/rimraf": "^3.0.2",
		"@types/update-notifier": "^4.1.0",
		"@typescript-eslint/eslint-plugin": "^2.33.0",
		"@typescript-eslint/parser": "^2.33.0",
		"babel-jest": "^27.1.0",
		"cz-conventional-changelog": "3.2.0",
		"dotenv-cli": "^4.0.0",
		"env-paths": "^2.2.0",
		"eslint": "^7.32.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-prettier": "^4.0.0",
		"jest": "^26.0.1",
		"prettier": "^2.3.2",
		"release-it": "^14.11.8",
		"ts-node": "^8.10.2",
		"tsup": "^5.10.3",
		"type-fest": "^2.5.2",
		"typescript": "^4.1.2"
	},
	"config": {
		"commitizen": {
			"path": "./node_modules/cz-conventional-changelog"
		}
	}
}